{
    "Role": {
        "Path": "/",
        "RoleName": "GithubActionsBuildAndPush",
        "RoleId": "AROASRVZUEL2AWVF6IUAT",
        "Arn": "arn:aws:iam::175409210100:role/GithubActionsBuildAndPush",
        "CreateDate": "2023-05-04T16:11:04+00:00",
        "AssumeRolePolicyDocument": {
            "Version": "2012-10-17",
            "Statement": [
                {
                    "Effect": "Allow",
                    "Principal": {
                        "Federated": "arn:aws:iam::175409210100:oidc-provider/token.actions.githubusercontent.com"
                    },
                    "Action": "sts:AssumeRoleWithWebIdentity",
                    "Condition": {
                        "ForAllValues:StringEquals": {
                            "token.actions.githubusercontent.com:aud": "sts.amazonaws.com"
                        },
                        "ForAnyValue:StringLike": {
                            "token.actions.githubusercontent.com:sub": "repo:APSPhysics/meetings-api:*"
                        }
                    }
                }
            ]
        },
        "Description": "Role for pushing ECR images in Github Actions",
        "MaxSessionDuration": 3600,
        "Tags": [
            {
                "Key": "IACPlatform",
                "Value": "none"
            },
            {
                "Key": "Environment",
                "Value": "prod"
            },
            {
                "Key": "CostCenter",
                "Value": "hgit"
            }
        ],
        "RoleLastUsed": {}
    }
}
